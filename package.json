{
  "name": "@paypal/sdk-client",
  "version": "4.0.168",
  "description": "Shared config between PayPal/Braintree.",
  "main": "index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "babel": "babel ./server --ignore=node_modules --out-dir ./server",
    "build": "npm run test && npm run doc",
    "clean": "rimraf dist coverage",
    "debug": "cross-env NODE_ENV=debug",
    "doc": "esdoc",
    "tsc": "tsc",
    "karma": "cross-env NODE_ENV=test babel-node --plugins=transform-es2015-modules-commonjs ./node_modules/.bin/karma start",
    "lint": "eslint --ext ts,tsx,js,jsx src/ test/",
    "release": "./publish.sh",
    "release:major": "./publish.sh major",
    "release:minor": "./publish.sh minor",
    "release:patch": "./publish.sh patch",
    "setup": "npm install && npm run tsc",
    "format": "prettier --write --ignore-unknown .",
    "format:check": "prettier --check .",
    "test": "npm run format:check && npm run lint && npm run tsc && npm run jest && npm run karma",
    "webpack": "babel-node --plugins=transform-es2015-modules-commonjs ./node_modules/.bin/webpack --progress",
    "jest": "jest test/server --env=node --no-cache --collectCoverageFrom='server/' --coverageDirectory='coverage/jest' --coverage --verbose --runInBand --silent=false",
    "prepublishOnly": "npm run babel",
    "postpublish": "rm -rf ./server && git checkout ./server",
    "validate-codecov": "curl --data-binary @.github/codecov.yml https://codecov.io/validate",
    "prepare": "husky install"
  },
  "files": [
    "dist/",
    "src/",
    "server/",
    "__sdk__.js",
    "test/globals.js"
  ],
  "browserslist": [
    "IE >= 11",
    "chrome >= 27",
    "firefox >= 30",
    "safari >= 7",
    "opera >= 23"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/paypal/paypal-sdk-client.git"
  },
  "keywords": [
    "template"
  ],
  "license": "Apache-2.0",
  "readmeFilename": "README.md",
  "dependencies": {
    "@krakenjs/beaver-logger": "6.0.0-typescript.1",
    "@krakenjs/belter": "3.0.0-typescript.1",
    "@krakenjs/cross-domain-utils": "4.0.0-typescript.1",
    "@krakenjs/jsx-pragmatic": "^3.0.0",
    "@krakenjs/zalgo-promise": "3.0.0-typescript.1",
    "@paypal/sdk-constants": "2.0.0-typescript.1",
    "bowser": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@krakenjs/babel-config-grumbler": "^8.0.7",
    "@krakenjs/eslint-config-grumbler": "^8.0.7",
    "@krakenjs/grumbler-scripts": "^8.0.5",
    "@krakenjs/sync-browser-mocks": "^3.0.0",
    "@krakenjs/typescript-config-grumbler": "^8.1.0",
    "@krakenjs/webpack-config-grumbler": "^8.1.0-alpha.1",
    "@types/jest": "^29.2.4",
    "@vitest/coverage-c8": "^0.25.3",
    "@vitest/ui": "^0.25.3",
    "babel-core": "7.0.0-bridge.0",
    "cheerio": "1.0.0-rc.9",
    "cross-env": "^7.0.3",
    "esdoc": "1.1.0",
    "esdoc-flow-type-plugin": "1.1.0",
    "esdoc-standard-plugin": "1.0.0",
    "flow-bin": "0.155.0",
    "flowgen": "^1.20.1",
    "happy-dom": "^7.7.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "mocha": "^4.1.0",
    "prettier": "2.7.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "4.9.3"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
